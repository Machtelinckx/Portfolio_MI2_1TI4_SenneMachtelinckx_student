                                        function Rekenmachine(){
                                            this.waarde1 =0;
                                            this.waarde2 =0;
							
                                            this.TelOp = function(){
                                                return this.waarde1 + this.waarde2;
                                            }
                                            this.Vermenigvuldigen = function(){
                                                return this.waarde1 * this.waarde2;
                                            }
                                            this.Verminder = function(){
                                                return this.waarde1 - this.waarde2;
                                            }
                                            this.Deel = function(){
                                                return this.waarde1 / this.waarde2;
                                            }
                                        }
                                        var rekenen = new Rekenmachine();
                                        rekenen.waarde1 = 5;
                                        rekenen.waarde2 = 10;
                                        var uitkomst;
                                        uitkomst = rekenen.Vermenigvuldigen(); 
	
                                        document.write("De uitkomst is : " + uitkomst);
                                        
                                       
				</script>
							
